    "grades = [85, 90, 78, 92, 88]\n",
    "\n",
    "mean = sum([ grade/len(grades) for grade in grades ])\n",
    "best = max(grades)\n",
    "worst = min(grades)\n",
    "honor_students = len([ _ for grade in grades if grade >= mean ])\n",
    "\n",
    "print(f\"\"\"\\\n",
    "平均点: {mean} \n",
    "最高点: {best}\n",
    "最低点: {worst}\n",
    "平均以上の学生数: {honor_students}   \n",
    "\"\"\")"
   ]
